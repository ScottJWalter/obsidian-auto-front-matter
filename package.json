  ],
  "license": "MIT",
  "devDependencies": {
    "@types/lodash-es": "^4.17.8",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "nanoid": "^4.0.2"
  }
}
