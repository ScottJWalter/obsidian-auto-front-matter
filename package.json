{
  "name": "obsidian-auto-front-matter",
  "version": "1.0.3",
  "description": "Auto update front matter",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
